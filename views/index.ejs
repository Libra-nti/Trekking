              <path d="M4 13.5a4 4 0 1 0 4 0v-8.5a2 2 0 1 0 -4 0v8.5" />
              <path d="M4 9h4" />
              <path d="M13 16a4 4 0 1 0 0 -8a4.07 4.07 0 0 0 -1 .124" />
              <path d="M13 3v1" />
              <path d="M21 12h1" />
              <path d="M13 20v1" />
              <path d="M19.4 5.6l-.7 .7" />
              <path d="M18.7 17.7l.7 .7" />
            </svg>
          </span>
          <input id="season" type="text" class="form-control dropdown-toggle" placeholder="Stagione" data-bs-toggle="dropdown" aria-expanded="false">
          <ul class="dropdown-menu" aria-labelledby="dropdownInput">
            <li>
              <a class="dropdown-item" href="#" onclick="season('Estate')">Estate</a>
            </li>
            <li>
              <a class="dropdown-item" href="#" onclick="season('Autunno')">Autunno</a>
            </li>
            <li>
              <a class="dropdown-item" href="#" onclick="season('Inverno')">Inverno</a>
            </li>
            <li>
              <a class="dropdown-item" href="#" onclick="season('Primavera')">Primavera</a>
            </li>
          </ul>
        </div>
        <div class="input-group mb-2">
          <button type="submit" class="btn btn-link" aria-label="Applica filtri">Applica</button>
        </div>
      </form>
    </div>
    <div id="content">
      <div id="trekking-list" class="container">
        <div class="row" id="row-1"><% treks.forEach(trek => {%> <div class="col-sm-4 col-lg-2 col-6 mb-3">
            <a href="/trekking/<%=trek.name%>" style="all: unset">
              <div class="card h-100" style="width: auto">
                <img loading="lazy" srcset="
    public/<%=trek.name%>-<%=trek.date%>/copertina-400.jpg 400w,
    public/<%=trek.name%>-<%=trek.date%>/copertina-800.jpg 800w,
    public/<%=trek.name%>-<%=trek.date%>/copertina-1200.jpg 1200w
  " sizes="(min-width: 992px) 16.66vw, (min-width: 576px) 33.33vw, 50vw" fetchpriority="high" src="public/<%=trek.name%>-<%=trek.date%>/copertina-400.jpg" class="card-img-top" name="<%=trek.name%>" alt="Foto copetina <%=trek.name%>">
                <div class="card-body bottom-0">
                  <h5 class="card-title"><%=trek.name%> </h5>
                  <div class="card-text">
                    <p><%=trek.date%></p>
                  <p class="hide-description" style="display: none;">
                    <%=trek.description%>
                  </p> </div>
                </div>
              </div>
            </a>
          </div><%});%> </div>
